<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="colley.chisholm.diploma.welcome"
    android:versionCode="1"
    android:versionName="1.0" >
    
     <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:name="colley.chisholm.diploma.welcome.settings.AppSettingz"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="colley.chisholm.diploma.welcome.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="colley.chisholm.diploma.welcome.UserHomeActivity"
            android:label="@string/title_activity_display_message"
            android:parentActivityName="colley.chisholm.diploma.welcome.MainActivity" >
        </activity>
        <activity
            android:name="colley.chisholm.diploma.welcome.ParticleActivity"
            android:label="@string/title_activity_particle" >
        </activity>
        <activity
            android:name="colley.chisholm.diploma.welcome.MapActivity"
            android:label="@string/title_activity_map" >
        </activity>
    </application>

</manifest>
